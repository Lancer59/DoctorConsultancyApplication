        <activity
            android:name=".ASurgeonPage"
            android:exported="false" />
        <activity
            android:name=".APsychiatristPage"
            android:exported="false" />
        <activity
            android:name=".AGeneralPage"
            android:exported="false" />
        <activity
            android:name=".SendMail"
