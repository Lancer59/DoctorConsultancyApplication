        <activity
            android:name=".AddGeneral"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="General" />
        <activity
            android:name=".AddPsychiatrist"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="Psychiatrist" />
        <activity
            android:name=".AddSurgeon"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="Surgeon" />
        <activity
            android:name=".AdminLogin"
            android:exported="false"
            android:icon="@drawable/w1"
            android:label="ADMIN LOGIN" />
        <activity
            android:name=".SurgeonPage"
            android:exported="false"
            android:icon="@drawable/w1"
